// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Car {
  id       Int    @id @default(autoincrement())
  maker    String
  model    String
  package  String
  color    String
  year     String
  category String
  mileage  Int
  price    Int
  imgUrl   String
}
